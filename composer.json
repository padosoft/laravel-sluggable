{
    "name": "padosoft/laravel-sluggable",
    "description": "Generate slugs when saving Eloquent models",
    "keywords": [
        "padosoft",
        "laravel-sluggable"
    ],
    "homepage": "https://github.com/padosoft/laravel-sluggable",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorenzo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "illuminate/database": "^5.7",
        "illuminate/support": "^5.7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\Sluggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\Sluggable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
